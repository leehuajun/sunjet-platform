                                                  emptyMessage="没有数据">
                                                <columns>
                                                    <column hflex="1" label="服务站编号"/>
                                                    <column hflex="2" label="服务站名称"/>
                                                </columns>
                                                <template name="model">
                                                    <row onClick="@command('selectDealer',model=each)">
                                                        <label value="@load(each.code)"/>
                                                        <label value="@load(each.name)"/>
                                                    </row>
                                                </template>
                                            </grid>
                                        </vlayout>
                                    </bandpopup>
                                </bandbox>
                                <label value="服务站名称"></label>
                                <textbox hflex="1" value="@bind(vm.dealerBackupInfo.name)" readonly="true"/>
                                <label value="法人代表" hflex="1"></label>
                                <textbox value="@load(vm.dealerBackupInfo.legalPerson)" hflex="1"
                                         readonly="true"></textbox>
                                <label value="工商营业执照号" hflex="1"></label>
