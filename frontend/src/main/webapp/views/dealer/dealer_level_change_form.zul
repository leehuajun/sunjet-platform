                                <textbox value="@load(vm.dealerBackupInfo.taxpayerCode)" hflex="1"
                                         readonly="true"></textbox>
                                <label value="开户银行" hflex="1"></label>
                                <textbox value="@load(vm.dealerBackupInfo.bank)" hflex="1" readonly="true"></textbox>
                                <label value="银行账号"></label>
                                <textbox value="@load(vm.dealerBackupInfo.bankAccount)" hflex="1"
                                         readonly="true"></textbox>
                            </row>
                            <row>
                                <label value="地址"></label>
                                <textbox value="@load(vm.dealerBackupInfo.address)" hflex="1" readonly="true"></textbox>
                                <label value="所在省"></label>
                                <textbox value="@load(vm.dealerBackupInfo.provinceName)" hflex="1"
                                         readonly="true"></textbox>
                                <label value="所在市"></label>
                                <textbox value="@load(vm.city.name)" hflex="1" readonly="true"></textbox>
