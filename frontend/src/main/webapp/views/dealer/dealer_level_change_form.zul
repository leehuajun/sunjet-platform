<zk>
    <borderlayout
            width="100%" height="100%"
            viewModel="@id('vm') @init('com.sunjet.frontend.vm.dealer.DealerLevelChangeFormVM')">
        <center sclass="form-center" hflex="true" vflex="true" autoscroll="true">
            <vlayout height="100%">
                <div sclass="include">
                    <include src="/views/commons/flow_form_toolbar.zul"/>
                    <include src="/views/commons/handle_win.zul"/>
                </div>
                <groupbox>
                    <grid oddRowSclass="none" hflex="1" sclass="form-grid">
                        <columns>
                            <column width="70px"/>
                            <column hflex="1"/>
                            <column width="80px"/>
                            <column hflex="1"/>
                            <column width="80px"/>
                            <column hflex="1"/>
                            <column width="80px"/>
                            <column hflex="1"/>
                        </columns>
                        <rows>
                            <row>
                                <label value="服务站编号" hflex="1"></label>
                                <bandbox mold="rounded" autodrop="false" hflex="1" readonly="true"
                                         buttonVisible="@load(!vm.readonly)"
                                         value="@load(vm.dealerBackupInfo.code)">
                                    <bandpopup width="300px" height="300px">
                                        <vlayout hflex="1" vflex="1">
                                            <textbox value="@bind(vm.keyword)" hflex="1"
                                                     placeholder="输入关键字..按Enter键搜索"
                                                     sclass="input-prepend"
                                                     onOK="@command('searchDealers',keyword=vm.keyword)"/>
